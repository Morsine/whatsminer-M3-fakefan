# this is a system file containing fan speed values to be used by the cgminer
config common 'common'
    option count        '2'

config fan 'fan0'
    list 'speed_node'   '200:6000'
    list 'speed_node'   '200:6000'
    list 'speed_node'   '200:6000'
    option speed_vs_freq_times '30'

config fan 'fan1'
    list 'speed_node'   '200:6000'
    list 'speed_node'   '200:6000'
    list 'speed_node'   '200:6000'
    option speed_vs_freq_times '30'
